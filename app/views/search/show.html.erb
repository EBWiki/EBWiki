<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <h2 class="page-header"><%= @presenter.search_results_text %></h2>
      <div id='front-gallery'>
        <%= render partial: 'cases/thumbnail', layout: "cases/index_thumbnail", collection: @cases, as: :this_case %>
      </div>
      <%= paginate @cases %>
      <p><%= page_entries_info @cases %></p>
    </div>
  </div>
</div>
